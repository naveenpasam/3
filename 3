#include<stdio.h>
main()
{
int a[20],i,n,s,flag=0;
printf("enter the no elements of array");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("enter %d element of array :",i+1);
scanf("%d",&a[i]);
}
printf("enter the element to search:");
scanf("%d",&s);
for(i=0;i<n;i++)
{
if(a[i]==s)
{
printf("element found");
flag=1;
}
}
if(flag==0)
printf("element not found");
}
